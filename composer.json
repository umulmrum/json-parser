{
    "name": "umulmrum/json-parser",
    "description": "A library that parses JSON as stream.",
    "keywords": [
        "json",
        "parser",
        "stream"
    ],
    "prefer-stable": true,
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Kruppa",
            "email": "stefan@umulmrum.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "umulmrum\\JsonParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "umulmrum\\JsonParser\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "symfony/polyfill-mbstring": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0",
        "psalm/phar": "^4.6"
    }
}
